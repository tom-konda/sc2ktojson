{
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "no-var-keyword": true,
    "no-consecutive-blank-lines": [],
    "no-constant-condition": true,
    "no-trailing-whitespace": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "triple-equals": [
      true
    ],
    "whitespace": [
      "check-branch",
      "check-operator",
      "check-separator"
    ]
  }
}